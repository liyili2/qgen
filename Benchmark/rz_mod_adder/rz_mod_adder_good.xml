<root>
<let> <id> adder </id> <id > x </id> <id > n </id> <id > size </id> <id > M </id> 
   <match> <id> n </id>
          <pair> <vexp> 0 </vexp> <pexp gate = 'SKIP' > <id> x </id> <vexp> 0 </vexp> </pexp> </pair>
           <pair> <vexp op = '+' > <id> m </id> <vexp> 1 </vexp> </vexp> 
               < app > <id> adder </id> <id> x </id> <id> m </id> <id> size </id> <id> M </id> </app> 
               < if > <vexp op = '$' > <id> M </id> <id> m </id> </vexp>
                      <pexp gate = 'SR' > <vexp op = '-' > <id> size </id> <id> m </id> </vexp> <id> x </id> </pexp>
                      <pexp gate = 'SKIP' > <id> x </id> <id> m </id> </pexp>
               </if> 
           </pair>   
   </match>
</let>

<let> <id> sub </id> <id> x </id> <id> n </id> <id > size </id> <id > M </id> 
   <match> <id> n </id>
          <pair> <vexp> 0 </vexp> <pexp gate = 'SKIP' > <id> x </id> <vexp> 0 </vexp> </pexp> </pair>
           <pair> <vexp op = '+' > <id> m </id> <vexp> 1 </vexp> </vexp> 
               < app > <id> sub </id> <id> x </id> <id> m </id> <id> size </id> <id> M </id> </app> 
               < if > <vexp op = '$' > <id> M </id> <id> m </id> </vexp>
                      <pexp gate = 'SR' > <vexp op = '-' > <id> m </id> <id> size </id> </vexp> <id> x </id> </pexp>
                      <pexp gate = 'SKIP' > <id> x </id> <id> m </id> </pexp>
               </if> 
           </pair>   
   </match>
</let>

<let> <id> f </id> <id> x </id> <id> n </id> <id> c </id> <id> a </id> <id> m </id> 
  <app> <id> adder </id> <id> x </id> <id> n </id> <id> n </id> <id> a </id> </app>
    <app> <id> sub </id> <id> x </id> <id> n </id> <id> n </id> <id> m </id> </app>
    <pexp gate = 'RQFT' > <id> x </id> </pexp>
        <pexp gate = 'CU' > <id> x </id>  <vexp op = '-' > <id> n </id> <vexp> 1 </vexp> </vexp> <pexp gate = 'X' > <id> c </id>  <vexp> 0 </vexp> </pexp></pexp>
    <pexp gate = 'QFT' > <id> x </id> <vexp> 0 </vexp> </pexp>
    <pexp gate = 'CU' > <id> c </id>  <vexp> 0 </vexp> <app> <id> adder </id> <id> x </id> <id> n </id> <id> n </id> <id> m </id> </app> </pexp>
      <app> <id> sub </id> <id> x </id> <id> n </id> <id> n </id> <id> m </id> </app>
    <pexp gate = 'RQFT' > <id> x </id> </pexp>
    <pexp gate = 'X' > <id> x </id>  <vexp op = '-' > <id> n </id> <vexp> 1 </vexp> </vexp> </pexp>
        <pexp gate = 'CU' > <id> x </id> <vexp op = '-' > <id> n </id> <vexp> 1 </vexp> </vexp> <pexp gate = 'X' > <id> c </id>  <vexp> 0 </vexp> </pexp></pexp>
     <pexp gate = 'X' > <id> x </id>  <vexp op = '-' > <id> n </id> <vexp> 1 </vexp> </vexp> </pexp>
    <pexp gate = 'QFT' > <id> x </id> <vexp> 0 </vexp> </pexp>
    <app> <id> adder </id> <id> x </id> <id> n </id> <id> n </id> <id> m </id> </app>
</let>

<pexp gate = 'QFT' > <id> x </id> <vexp> 0 </vexp> </pexp> 
< app > <id> f </id> <id> x </id> <id> na </id> <id> c </id> <id> a </id> <id> m </id> </app> 
<pexp gate = 'RQFT' > <id> x </id> </pexp>
</root>

